# Agent Manager Environment Variables
# Copy this file to .env and adjust the values according to your setup

# Web Server Settings
AGENT_MANAGER_HOST=0.0.0.0
AGENT_MANAGER_PORT=5000
AGENT_MANAGER_DEBUG=True
AGENT_MANAGER_SECRET_KEY=change_this_secret_key_in_production

# Docker Settings
AGENT_MANAGER_DOCKER_URL=unix://var/run/docker.sock
AGENT_MANAGER_BASE_IMAGE=consol/rocky-xfce-vnc
AGENT_MANAGER_NETWORK_NAME=agent-network

# VNC Settings
AGENT_MANAGER_VNC_PASSWORD=vncpassword
AGENT_MANAGER_VNC_RESOLUTION=1280x800
AGENT_MANAGER_VNC_PORT_MIN=5901
AGENT_MANAGER_VNC_PORT_MAX=5910
AGENT_MANAGER_NOVNC_PORT_MIN=6901
AGENT_MANAGER_NOVNC_PORT_MAX=6910

# Redis Settings
AGENT_MANAGER_REDIS_HOST=redis
AGENT_MANAGER_REDIS_PORT=6379
AGENT_MANAGER_REDIS_DB=0

# AI Settings
AGENT_MANAGER_AI_API_KEY=your_anthropic_api_key_here
AGENT_MANAGER_AI_MODEL=claude-3-opus-20240229
AGENT_MANAGER_AI_MAX_RETRIES=3

# Session Settings
AGENT_MANAGER_MAX_SESSIONS=5

# Logging Settings
AGENT_MANAGER_LOG_LEVEL=INFO